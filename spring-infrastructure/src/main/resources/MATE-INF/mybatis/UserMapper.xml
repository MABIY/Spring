<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chinal.lh.infrastructure.mapper.UserMapper">
    <resultMap id="BaseResultMap" type="com.chinal.lh.domain.data.User">

    </resultMap>
    <select id="getUser" resultMap="BaseResultMap" parameterType="java.lang.Long">
        select *
        from user
        where id =#{id}
    </select>

